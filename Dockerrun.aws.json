{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "node:14",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "3000"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current",
      "ContainerDirectory": "/usr/src/app"
    }
  ],
  "Environment": [
    {
      "Name": "REACT_APP_API_URL",
      "Value": "https://ec66-146-70-202-5.ngrok-free.app"
    }
  ],
  "Entrypoint": "sh -c \"npm install && npm run build && npx serve -s build\""
}
